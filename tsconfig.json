{
    "compilerOptions": {
        "target": "ES2019",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "declaration": true,
        "emitDeclarationOnly": false,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "outDir": "dist",
        "types": [
            "react",
            "react-dom"
        ]
    },
    "include": [
        "src"
    ]
}